        <mj-style>
            /* DARK MODE */
            :root {
            color-scheme: light dark;
            supported-color-schemes: light dark;
            }
            @media (prefers-color-scheme: dark) {
            .dark-mode-bg>table {
            background: #272727 !important;
            background-image: linear-gradient(#272727, #272727) !important;
            }
            .dark-mode-hero-bg>table {
            background: #3d3d3d !important;
            background-image: linear-gradient(#3d3d3d, #3d3d3d) !important;
            }
            .dark-mode-callout {
            background: #272727 !important;
            background-image: linear-gradient(#272727, #272727) !important;
            }
            .dark-mode-social-bg>table {
            background: #3d3d3d !important;
            background-image: linear-gradient(#3d3d3d, #3d3d3d) !important;
            }
            .dark-mode-text {
            color: #ffffff !important;
            -webkit-text-fill-color: #ffffff !important;
            }
            .dark-mode-hero-text {
            color: #fec800 !important;
            -webkit-text-fill-color: #fec800 !important;

            }
            .dark-mode-CTA-text {
            color: #333333 !important;
            -webkit-text-fill-color: #333333 !important;
            }
            .dark-mode-CTA {
            background: #f5f5f5 !important;
            background-image: linear-gradient(#f5f5f5, #f5f5f5) !important;
            }
            /* Shows Dark Mode-Only Content, Like Images */
            .dark-img { 
            display:block !important; 
            }
            /* Hides Light Mode-Only Content, Like Images */
            .light-img { 
            display:none !important; 
            }

            }
            [data-ogsb] .dark-mode-bg>table {
            background: #272727 !important;
            }
            [data-ogsb] .dark-mode-hero-bg>table {
            background: #3d3d3d !important;
            }
            [data-ogsb] .dark-mode-callout {
            background: #272727 !important;
            }
            [data-ogsb] .dark-mode-social-bg>table {
            background: #3d3d3d !important;
            }
            [data-ogsc] .dark-mode-text {
            color: #ffffff !important;
            }
            [data-ogsc] .dark-mode-hero-text {
            color: #fec800 !important;

            }
            [data-ogsc] .dark-mode-CTA-text {
            color: #333333 !important;
            }
            [data-ogsb] .dark-mode-CTA {
            background: #f5f5f5 !important;
            }
            /* Shows Dark Mode-Only Content, Like Images */
            [data-ogsc] .dark-img { 
            display:block !important; 
            width: auto !important; 
            overflow: visible !important; 
            float: none !important; 
            max-height:inherit !important; 
            max-width:inherit !important; 
            line-height: auto !important; 
            margin-top:0px !important; 
            visibility:inherit !important; 
            }
            /* Hides Light Mode-Only Content, Like Images */
            [data-ogsc] .light-img { 
            display:none; 
            display:none !important; 
            }
        </mj-style>
