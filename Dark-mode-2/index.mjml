    <mj-raw>
        <main>
    </mj-raw>
        <mj-section background-color="#D8D8D8" padding="10px 0" css-class="dark-mode-preheader-bg">
            <mj-column>
                <mj-text font-size="10px" line-height="12px" color="#333333" padding="4px 0 0 20px" css-class="dark-mode-preheader-text">
                     Don&rsquo;t forget to plan ahead when traveling with your treatment
                </mj-text>
            </mj-column>
            <mj-column>
                <mj-text font-size="10px" line-height="12px" color="#333333" padding="4px 20px 0 20px" css-class="dark-mode-preheader-text">
                    <p class="textRight" style="margin: 0;"><a href="#" target="_blank" style="font-family: Helvetica, Arial, sans-serif; font-size: 10px; line-height:12px; color: #333333; text-decoration: none;">View this email in your browser &#187;</a></p>
